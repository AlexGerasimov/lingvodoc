<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="favicon.ico">

    <title>Lingvodoc future</title>
    <link rel="stylesheet" type="text/css" href="${request.static_url('lingvodoc:static/css/lingvodoc.css')}"/>
    <script src="${request.static_url('lingvodoc:static/js/lingvodoc.js')}"></script>

</head>

<body class="body-dashboard" ng-app="BlobUploadModule">

<nav class="lingvodoc-navbar">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                    aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">Lingvodoc</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li><a href="${request.route_url('home')}">Home</a></li>
                <li tal:condition="user"><a href="${request.route_url('dashboard')}">Dictionaries</a></li>
                <li tal:condition="user"><a href="${request.route_url('languages')}">Languages</a></li>
                <li><a href="#contact">Contact</a></li>
                <li><a href="#about">About</a></li>
            </ul>

            <ul class="nav navbar-nav navbar-right">

                <li class="dropdown" tal:condition="user">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true"
                       aria-expanded="false">${user.name}<span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="#" onclick="alert('Not implemented!');">Profile</a></li>
                        <li><a href="${request.route_url('logout')}">Sign out</a></li>
                    </ul>
                </li>

                <li class="dropdown" tal:condition="not: user">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true"
                       aria-expanded="false">Guest<span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="${request.route_url('login')}">Sign in</a></li>
                        <li><a href="${request.route_url('signup')}">Sign up</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
</nav>


<div class="container" ng-controller="BlobUploadController">


    <form action="/blob" method="post" accept-charset="utf-8" enctype="multipart/form-data">

        <label for="mp3">Mp3</label>
        <input id="mp3" name="blob" type="file" value="" />
        <input type="hidden" name="data_type" value="dialeqt_dictionary">

        <input type="submit" value="submit" />
    </form>
    


</div>

</body>

</html>